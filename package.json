{
  "name": "@jtwebb/auth",
  "version": "1.0.0",
  "description": "Authentication library for web applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./pg": {
      "types": "./dist/adapters/pg/index.d.ts",
      "import": "./dist/adapters/pg/index.js"
    },
    "./kysely": {
      "types": "./dist/adapters/kysely/index.d.ts",
      "import": "./dist/adapters/kysely/index.js"
    },
    "./react-router": {
      "types": "./dist/adapters/react-router/index.d.ts",
      "import": "./dist/adapters/react-router/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pg:integration": "AUTH_PG_INTEGRATION=1 vitest run",
    "test:kysely:integration": "AUTH_KYSELY_INTEGRATION=1 vitest run",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && node scripts/copy-migrations.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "authentication",
    "web",
    "library"
  ],
  "author": "John Webb",
  "license": "MIT",
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "kysely": "^0.28.0",
    "pg": "^8.16.3",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.39.2",
    "kysely": "^0.28.0",
    "pg": "^8.16.3",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "testcontainers": "^11.7.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2"
  }
}
